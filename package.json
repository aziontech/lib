      "require": "./packages/utils/dist/node.cjs",
      "import": "./packages/utils/dist/node.mjs"
    },
    "./domains": {
      "types": "./packages/domains/dist/index.d.ts",
      "require": "./packages/domains/dist/index.cjs",
      "import": "./packages/domains/dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "ai": [
        "./packages/ai/dist/index.d.ts"
      ],
      "config": [
        "./packages/config/dist/index.d.ts"
      ],
      "config/rules": [
