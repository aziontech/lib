{
  "name": "@lib/presets",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "scripts": {
    "compile": "vite build --config vite.config.mjs && vite build --config vite.config.presets.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "jest --clearCache && NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./preset/nuxt/*": "./src/presets/nuxt/nitro/*/index.js",
    "./preset/sveltekit/*": "./src/presets/svelte/kit/index.js",
    "./preset/sveltekit/cache/*": "./src/presets/svelte/kit/cache/index.js",
    "./presets/preset/*": "./dist/presets/*"
  },
  "author": "aziontech",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "src/presets/next/*",
    "src/presets/nuxt/nitro/*",
    "src/presets/svelte/kit/*"
  ],
  "dependencies": {
    "cookie": "^1.0.2",
    "fast-glob": "^3.3.3",
    "pcre-to-regexp": "^1.1.0",
    "webpack": "^5.97.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0"
  }
}
