  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "azion/unenv-preset": ["../unenv-preset/src/index.ts"]
    }
  },
  "exclude": ["node_modules"]
